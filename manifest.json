{
  "manifest_version": "1",
  "type": "WebApp",
  "meta": {
    "name": "Valorant Record at death",
    "version": "0.0.1",
    "minimum-overwolf-version": "0.92.300.0",
    "author": "takepiyo",
    "icon": "icon.png",
    "icon_gray": "icon_gray.png",
    "description": "record at death"
  },
  "permissions": [
    "Extensions",
    "FileSystem",
    "Streaming",
    "DesktopStreaming"
  ],
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        21640
      ]
    },
    "externally_connectable": {
      "matches": [
        "http://localhost",
        "http://localhost:8001",
        "http://localhost:8010",
        "https://social-login-sample-server.overwolf.com"
      ]
    },
    "start_window": "index",
    "windows": {
      "index": {
        "file": "index.html",
        "transparent": true,
        "clickthrough": true,
        "resizable": false,
        "show_in_taskbar": true,
        "allow_local_file_access": true,
        "size": {
          "width": 1150,
          "height": 535
        },
        "start_position": {
          "Top": 10,
          "Left": 10
        }
      }
    },
    "game_events": [21640],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            21640
          ]
        },
        "start_minimized": false
      }]
}
}
